<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre_id %>

  <%= f.text_field :genre_id, list: "genres_autocomplete" %>

  <datalist id="genres_autocomplete">
    <% Genre.all.each do |genre| %>
      <option value="<%= genre.id %>" >
    <% end %>
  </datalist>

  <br>
  <input name="song[notes_contents][]" id="song_notes_1">

  <input name="song[notes_contents][]" id="song_notes_2">
  <br />

  <%= f.submit %>
<% end %>
